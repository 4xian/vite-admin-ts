{
  "name": "vite-admin-ts",
  "author": "4xian",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "prettier": "prettier \"{src,mock}/**/*.{vue,ts}\" --write",
    "lint:js": "eslint \"{src,mock}/**/*.{vue,ts}\" ",
    "lint:js-fix": "eslint \"{src,mock}/**/*.{vue,ts}\" --fix",
    "lint:css": "stylelint \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:css-fix": "stylelint \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/ --fix",
    "test": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run prettier",
      "npm run lint:js-fix",
      "npm run lint:css-fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@element-plus/icons": "0.0.11",
    "@vueuse/core": "^6.7.3",
    "axios": "0.23.0",
    "cropperjs": "^1.5.12",
    "echarts": "5.1.1",
    "element-plus": "^1.1.0-beta.19",
    "js-cookie": "2.2.1",
    "lodash-es": "^4.17.21",
    "mockjs": "1.1.0",
    "nprogress": "0.2.0",
    "path": "^0.12.7",
    "pinia": "^2.0.0-rc.14",
    "screenfull": "5.1.0",
    "unplugin-vue-components": "^0.16.0",
    "vue": "^3.2.16",
    "vue-router": "^4.0.11",
    "vuex": "4.0.0",
    "wangeditor": "^4.7.9"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@types/jest": "26.0.22",
    "@types/js-cookie": "2.2.6",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "14.14.31",
    "@types/nprogress": "0.2.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/compiler-sfc": "3.1.2",
    "@vue/test-utils": "2.0.0-rc.6",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.21.0",
    "eslint-plugin-vue": "7.7.0",
    "husky": "7.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "sass": "1.32.8",
    "stylelint": "13.11.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "ts-jest": "26.5.5",
    "typescript": "^4.4.3",
    "vite": "2.6.4",
    "vite-plugin-mock": "2.4.2",
    "vue-jest": "5.0.0-alpha.7"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
